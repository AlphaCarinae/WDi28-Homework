<h1>New ocean</h1>

<form action="<%= oceans_path %>" method="post">
  <label>Name:</label>
  <input type="text" name="name" required>
  <label>Image Url:</label>
  <input type="url" name="image_url">
  <label>Average Depth:</label>
  <input type="number" name="average_depth">
  <button>Create ocean</button>
</form>
