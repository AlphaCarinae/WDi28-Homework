<!DOCTYPE html>
<html>
  <head>
    <title>OceansApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="containter">
      <nav>
        <%= link_to 'All Oceans', oceans_path %>
        <%= link_to 'New Ocean', oceans_new_path %>
      </nav>
      <%= yield %>
    </div>
  </body>
</html>
